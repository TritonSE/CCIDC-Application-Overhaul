{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "dist/index.js"
    }
  ],

  "headers": [
    {
      "source": "/*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://ccidc-application-overhaul-frontend.vercel.app"
        }
      ]
    }
  ]
}
