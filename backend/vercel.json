{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/dist/index"
    }
  ],

  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://ccidc-application-overhaul-frontend.vercel.app"
        }
      ]
    }
  ]
}
